package ripl;

struct rrt_goal_status_t {
       int64_t utime; 
       int32_t id;
       int8_t status; 

       const int8_t IDLE = 0x00; //no goal 
       const int8_t ACTIVE = 0x01; //going to goal 
       const int8_t REACHED = 0x02; //at goal               
       const int8_t FAILED = 0x03; //at goal                    
}