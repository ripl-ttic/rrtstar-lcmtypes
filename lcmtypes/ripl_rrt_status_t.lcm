package ripl;

struct rrt_status_t {

       int32_t iteration_no;      // number of iterations, i.e., samples, up until now
       int32_t num_nodes;         // number of nodes in the tree
       double best_node_cost;     // cost of the best path in the tree
       int32_t total_computation_time;    // cpu time up until now (microseconds)
       boolean reached_goal;
}
